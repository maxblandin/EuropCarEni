<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mbrb.eni.com.locationeuropcar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ecran.ConnexionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ecran.ListeActivity"
            android:label="@string/title_activity_liste"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ecran.LouerActivity" />
        <activity android:name=".ecran.ModifierAgenceActivity" />
    </application>

</manifest>